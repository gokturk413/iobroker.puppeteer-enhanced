    "useExternalBrowser": {
      "type": "checkbox",
      "label": "useExternalBrowser"
    },
    "executablePath": {
      "placeholder": "/usr/bin/chromium-browser",
      "hidden": "!data.useExternalBrowser",
      "newLine": true,
      "type": "text",
      "label": "executablePath"
    },
    "additionalArgs": {
      "newLine": true,
      "label": "additionalLaunchArgs",
      "type": "table",
      "uniqueColumns": ["Argument"],
      "xs": 12,
      "items": [{
        "attr": "Argument",
        "label": "Argument",
        "filter": false,
        "sort": false,
        "type": "autocomplete",
        "freeSolo": true,
        "options": [
          "--disable-lcd-text",
          "--disable-composited-antialiasing"
        ]
      }]
    },
    "_webLoginHeader": {
      "newLine": true,
      "type": "header",
      "text": "webLoginCredentials",
      "size": 5
    },
    "webUsername": {
      "newLine": true,
      "type": "text",
      "label": "webUsername",
      "placeholder": "admin"
    },
    "webPassword": {
      "newLine": true,
      "type": "password",
      "label": "webPassword",
      "visible": true,
      "repeat": false
    }
